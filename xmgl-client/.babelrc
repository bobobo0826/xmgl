{
    "presets": [
        "es2015",
        "stage-2",
        "react"
    ],
    "plugins": [
        ["transform-runtime"],
        [
            "transform-es2015-for-of"
        ],
        [
            "transform-class-properties"
        ],
        [
            "react-transform",
            {
                "transforms": [{
                    "transform": "react-transform-hmr",
                    "imports": [
                        "react"
                    ],
                    "locals": [
                        "module"
                    ]
                }]
            }
        ]
    ]
}